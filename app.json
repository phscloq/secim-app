{
  "expo": {
    "name": "secim-app",
    "slug": "secim-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "secim-app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "build": {
        "output": {
          "publicPath": "./"
        }
      }
    },
    "extra": {
      "eas": {
        "projectId": "316c31d3-55af-4e33-93c6-d5da6290451c"
      }
    }
  }
}
